
public class Controller {

	public static void main(String[] args) {
		TitleDescription titleDescription = new Window();// create JFrame window(?)
		titleDescription.setDefaultCloseOperation( JFrame.EXIT_ON_CLOSE );
		titleDescription.setSize( 600, 300 );
		titleDescription.setVisible( true ); 
		titleDescription.setResizable(true);
	}

}
